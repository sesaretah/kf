<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @visits.each do |visit| %>
          <tr>
                        <td><%= visit.visitable_id %></td>
                        <td><%= visit.visitable_type %></td>
                        <td><%= visit.visitor_session %></td>
                        <td><%= link_to t(:show), visit %></td>
            <td><%= link_to t(:edit), edit_visit_path(visit) %></td>
            <td><%= link_to t(:destroy), visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
